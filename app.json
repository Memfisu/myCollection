{
  "expo": {
    "name": "myCollection",
    "slug": "myCollection",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/myCollection_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/myCollection_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/myCollection_icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "MyCollection accesses your photos to upload an image",
          "cameraPermission": "MyCollection accesses your camera to upload an image"
        }
      ]
    ]
  }
}
